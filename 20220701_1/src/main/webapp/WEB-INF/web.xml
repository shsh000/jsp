<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">

	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>EmpListServ</servlet-name> <!-- 클래스 별칭 -->
		<servlet-class>co.micol.prj.EmpListServ</servlet-class> <!-- 패키지명.클래스명 -->
	</servlet>
	<servlet-mapping>
		<servlet-name>EmpListServ</servlet-name>
		<url-pattern>/listEmp</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EmpListServ</servlet-name>
		<url-pattern>/listEmp2</url-pattern>
	</servlet-mapping>
</web-app>